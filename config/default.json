{
    "db": {
        "dialect": "sqlite3",
        "useNullAsDefault": true,
        "connection": {
            "filename": "identifi.db"
        },
        "debug": false
    },
    "passport": {
      "facebook": {
        "clientID": null,
        "clientSecret": null,
        "callbackURL": "http://www.example.com/auth/facebook/callback"
      },
      "twitter": {
        "consumerKey": null,
        "consumerSecret": null,
        "callbackURL": "http://www.example.com/auth/twitter/callback"
      },
      "google": {
        "clientID": null,
        "clientSecret": null,
        "callbackURL": "http://www.example.com/auth/google/callback"
      },
      "persona": {
        "audience": null,
        "audience_info": "Set your node url as audience"
      }
    },
    "port": 4944,
    "host": "127.0.0.1",
    "ipfsHost": "127.0.0.1",
    "ipfsPort": 5001,
    "logfile": "identifi.log",
    "datadir": "~/.identifi",
    "maxMessageCount": 15000
}
