{
    "extends": "airbnb-base",
    "rules": {
        "no-console": 0,
        "radix": 0,
        "no-underscore-dangle": 0,
        "consistent-return": 0,
        "prefer-destructuring": 0,
        "import/no-extraneous-dependencies": 0,
        "import/no-dynamic-require": 0,
        "global-require": 0
    }
}
